{
  "name": "scully-master",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "scully": "scully",
    "scully:serve": "scully serve",
    "buildscully": "npm run build -- --configuration production --stats-json && npm run scully -- --scanRoutes --showGuessError",
    "scanscully": "npm run scully -- --scanRoutes --showGuessError",
    "copy404": "cp dist/soloCoding-blog/index.html dist/soloCoding-blog/404.html",
    "moveToDocs": "mv dist/static/* docs/",
    "buildGithub": "ng build --prod && npm run copy404 && npm run scanscully && npm run moveToDocs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.1",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/forms": "~12.0.1",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/router": "~12.0.1",
    "@scullyio/init": "^1.0.0",
    "@scullyio/ng-lib": "^1.0.0-beta.1",
    "@scullyio/scully": "^1.0.0-beta.5",
    "autoprefixer": "^10.4.0",
    "ngx-drag-scroll": "*",
    "postcss": "^8.3.11",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.1",
    "@angular/cli": "~12.0.1",
    "@angular/compiler-cli": "~12.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "3.8",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "tailwindcss": "^2.2.19",
    "typescript": "~4.2.3"
  }
}
